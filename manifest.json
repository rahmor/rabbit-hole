{
  "name": "Rabbit Hole",
  "description": "Clip a part of your web history and make a bookmark folder.",
  "version": "0.1.0",
  "manifest_version" : 3,
  "icons": { 
    "16":"./obj-16x16.png", 
    "32":"./obj-32x32.png", 
    "48":"./obj-48x48.png", 
    "128":"./obj-128x128.png" 
  },
  "action": {
    "default_popup":"rabbithole.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "activeTab",
    "declarativeContent",
    "history"
  ]
}