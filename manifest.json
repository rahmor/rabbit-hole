{
  "name": "Rabbit Hole",
  "description": "Clip a part of your web history and make a Bookmarks folder.",
  "version": "0.1.0",
  "manifest_version" : 3,
  "icons": { 
    "16":"./blackhole16.png", 
    "32":"./blackhole32.png", 
    "48":"./blackhole64.png", 
    "128":"./blackhole128.png" 
  },
  "action": {
    "default_popup":"rabbithole.html"
  },
  "permissions": [
    "bookmarks",
    "history"
  ]
}